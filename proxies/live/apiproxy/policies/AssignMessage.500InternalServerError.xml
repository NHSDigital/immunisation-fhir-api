<AssignMessage async="false" continueOnError="false" enabled="true" name="AssignMessage.500InternalServerError">
  <StatusCode>500</StatusCode>
  <Set>
    <Payload contentType="application/json">
    {Javascript.dynamic_response}
    </Payload>
  </Set>
  <Javascript name="Javascript_uuid">
    <ResourceURL>jsc://DynamicPayload.js</ResourceURL>
  </Javascript>
  <IgnoreUnresolvedVariables>false</IgnoreUnresolvedVariables>
  <AssignTo createNew="false" transport="http" type="request"/>
</AssignMessage>
