<AssignMessage async="false" continueOnError="false" enabled="true" name="AssignMessage.422InvalidResource">
  <StatusCode>422</StatusCode>
  <Set>
    <Payload contentType="application/json">
    {
      "resourceType": "OperationOutcome",
      "id": "{Javascript_uuid}",
      "meta": {
        "profile": [
          "https://simplifier.net/guide/UKCoreDevelopment2/ProfileUKCore-OperationOutcome"
        ]
      },
      "issue": [
        {
          "severity": "error",
          "code": "invalid_resource",
          "details": {
            "coding": [
              {
                "system": "https://fhir.nhs.uk/Codesystem/http-error-codes",
                "code": "INVALID_RESOURCE"
              }
            ]
          },
          "diagnostics": "Submitted resource is not valid."
        }
      ]
    }
    </Payload>
  </Set>
  <Javascript name="Javascript_uuid">
    <ResourceURL>jsc://uuid.js</ResourceURL>
  </Javascript>
  <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
  <AssignTo createNew="false" transport="http" type="request"/>
</AssignMessage>